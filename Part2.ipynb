   "source": [
    "import random\n",
    "num=range(0,100) #O(100)\n",
    "lis=random.sample(num,20)\n",
    "lis1=[n for n in lis if n%2==1]\n",
    "lis2=[n for n in lis if n%2==0]\n",
    "lis1.sort(reverse=True)#O(n log n)\n",
    "lis2.sort()#O(n log n)\n",
    "newlis=lis1+lis2\n",
    "print(newlis)"
   ]
